{
    "version": 2,
    "builds": [
        {
            "src": "/api/brand.js",
            "use": "@vercel/node"
        },
        {
            "src": "/api/categories.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/brand",
            "dest": "/api/brand.js"
        },
        {
            "src": "/api/categories",
            "dest": "/api/categories.js"
        }
    ]
}